function printEvenNumbers(min,max){
	var text = "";
	for (i = min; i < max; i++) {
		if(i%2 == 0){
			text += i+", ";
		}
	}
	document.write(text);
	console.log("finished");
}


function checkQuery(){
	var queryElement = document.getElementById("query");
	if (queryElement.value == "Java"){
		alert("Valid Query!");
	}
	else{
		alert("Invalid Query");
	}
}